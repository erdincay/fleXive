<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config
        PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

    <managed-bean>
        <managed-bean-name>feedbackPluginBean</managed-bean-name>
        <managed-bean-class>
            com.flexive.faces.beans.plugin.FeedbackPluginBean
        </managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

     <navigation-rule>
        <navigation-case>
            <from-outcome>feedback</from-outcome>
            <to-view-id>/feedback_plugin/feedback.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>

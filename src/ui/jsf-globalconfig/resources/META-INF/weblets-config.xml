<?xml version="1.0" encoding="UTF-8" ?>
<weblets-config xmlns="http://weblets.dev.java.net/config">
    <!-- Example weblet configuration -->
    <weblet>
        <weblet-name>divisions_plugin</weblet-name>
        <weblet-class>
            net.java.dev.weblets.packaged.PackagedWeblet
        </weblet-class>
        <weblet-version>@FLEXIVE_VERSION@</weblet-version>
        <init-param>
            <param-name>package</param-name>
            <param-value>
                com.flexive.faces.weblets
            </param-value>
        </init-param>
    </weblet>
    <weblet-mapping>
        <weblet-name>divisions_plugin</weblet-name>
        <url-pattern>/division-editor-resources/*</url-pattern>
    </weblet-mapping>
</weblets-config>
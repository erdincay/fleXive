<FindBugsFilter>
    <!-- Exclude autogenerated classes -->
    <Match>
        <Class name="~com.flexive.core.search.cmis.parser.CmisSqlLexer.*$"/>
    </Match>
    <Match>
        <Class name="~com.flexive.core.search.cmis.parser.CmisSqlParser.*$"/>
    </Match>


    <Match>
        <!--
            As of Findbugs 1.3.8, OBL_UNSATISFIED_OBLIGATION is marked experimental and reports all open result sets.
            These are closed by the JDBC driver when the statement that generated them is closed.
        -->
        <Bug pattern="OBL_UNSATISFIED_OBLIGATION"/>
    </Match>
    <Match>
        <!-- More complex queries are always constructed, however we also always use PreparedStatements -->
        <Bug pattern="SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE"/>
    </Match>
    <Match>
        <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING"/>
    </Match>

</FindBugsFilter>
